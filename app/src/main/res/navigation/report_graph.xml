<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/report_graph"
    app:startDestination="@id/reportLostFragment">

  <!--  <action
        app:enterAnim="@anim/fade_in"
        app:popExitAnim="@anim/slide_right"
        android:id="@+id/action_pop_out_of_report_auth_graph"
        app:destination="@+id/homeFragment"
        app:launchSingleTop="true"
        app:popUpTo="@id/reportAuthFragment"
        app:popUpToInclusive="true" />
-->
    <action
        app:enterAnim="@anim/fade_in"
        app:popExitAnim="@anim/slide_right"
        android:id="@+id/action_pop_out_of_report_graph"
        app:destination="@+id/homeFragment"
        app:launchSingleTop="true"
        app:popUpTo="@id/reportFragment"
        app:popUpToInclusive="true" />

    <fragment
        android:id="@+id/reportLostFragment"
        android:name="com.mafqud.android.report.lost.ReportLostFragment"
        android:label="ReportLostFragment" >
        <action
            android:id="@+id/action_reportLostFragment_to_reportLostSecondFragment"
            app:destination="@id/reportLostSecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/reportFoundFragment"
        android:name="com.mafqud.android.report.found.ReportFoundFragment"
        android:label="ReportFoundFragment" >
        <action
            android:id="@+id/action_reportFoundFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
        <action
            android:id="@+id/action_reportFoundFragment_to_reportfoundSecondFragment"
            app:destination="@id/reportfoundSecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/reportLostSecondFragment"
        android:name="com.mafqud.android.report.lost.ReportLostSecondFragment"
        android:label="ReportLostSecondFragment" >
        <action
            android:id="@+id/action_reportLostSecondFragment_to_uploadingImagesFragment"
            app:destination="@id/uploadingImagesFragment" />
        <argument
            android:name="imagesUrisPicked"
            app:argType="string[]" />
        <argument
            android:name="givID"
            app:argType="integer" />
        <argument
            android:name="cityID"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/cameraFragment"
        android:name="com.mafqud.android.camera.CameraFragment"
        android:label="CameraFragment" />
    <fragment
        android:id="@+id/reportfoundSecondFragment"
        android:name="com.mafqud.android.report.found.ReportfoundSecondFragment"
        android:label="ReportfoundSecondFragment" >
        <action
            android:id="@+id/action_reportfoundSecondFragment_to_uploadingImagesFragment"
            app:destination="@id/uploadingImagesFragment" />

        <argument
            android:name="imagesUrisPicked"
            app:argType="string[]" />
        <argument
            android:name="givID"
            app:argType="integer" />
        <argument
            android:name="cityID"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/uploadingImagesFragment"
        android:name="com.mafqud.android.report.uploading.UploadingImagesFragment"
        android:label="UploadingImagesFragment" >
        <argument
            android:name="imagesUrisPicked"
            app:argType="string[]" />
        <argument
            android:name="caseBody"
            app:argType="com.mafqud.android.report.uploading.models.CreateCaseBody" />
    </fragment>

</navigation>