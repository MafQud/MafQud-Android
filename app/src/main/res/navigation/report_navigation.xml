<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/report_navigation.xml"
    app:startDestination="@id/reportAuthFragment">

    <fragment
        android:id="@+id/reportAuthFragment"
        android:name="com.mafqud.android.report.report.ReportAuthFragment"
        android:label="ReportAuthFragment" >
        <action
            android:id="@+id/action_reportAuthFragment_to_reportLostFragment2"
            app:destination="@id/reportLostFragment2" />
        <action
            android:id="@+id/action_reportAuthFragment_to_reportFoundFragment2"
            app:destination="@id/reportFoundFragment2" />
    </fragment>
    <fragment
        android:id="@+id/reportLostFragment2"
        android:name="com.mafqud.android.report.lost.ReportLostFragment"
        android:label="ReportLostFragment" />
    <fragment
        android:id="@+id/reportFoundFragment2"
        android:name="com.mafqud.android.report.found.ReportFoundFragment"
        android:label="ReportFoundFragment" />
</navigation>